    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/timegrid": "^6.1.15",
    "@fullcalendar/vue3": "^6.1.15",
    "@popperjs/core": "^2.11.8",
    "@vue/compiler-sfc": "^3.5.12",
    "@vueuse/core": "^11.2.0",
    "dot": "^1.1.3",
    "dotenv": "^16.4.5",
    "env": "^0.0.2",
    "firebase": "^10.14.1",
    "fs": "^0.0.1-security",
    "graceful-fs": "^4.2.11",
