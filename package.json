{
  "name": "my-vue-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "configurations": [
    {  "command": "vite debug front",
       "name": "Debug Front-End",
       "type": "vite --inspect main.js",
       "request": "launch",
       "url": "http://chrome://inspect/#devices",
       "webRoot": "${workspaceFolder}/src",
       "port": 5050,
        "preLaunchTask": "vite"
       
    },
    {
       "type": "chrome",
       "request": "launch",
       "name": "Launch Frontend",
       "url": "http://localhost:3000",
       "webRoot": "${workspaceFolder}/src"
    }
 ],
     "dependencies": { 
